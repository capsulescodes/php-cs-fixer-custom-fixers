{
    "name" : "capsulescodes/php-cs-fixer-custom-fixers",
    "description" : "Capsules Codes set of custom fixers for PHP CS Fixer",
    "license": "MIT",
    "type" : "library",
    "authors" : [ { "name" : "Yannick Decat", "email" : "yannick@capsules.codes", "homepage" : "https://capsules.codes" } ],
    "require" : { "php" : "^8.3.0", "friendsofphp/php-cs-fixer" : "^3.88.2" },
    "require-dev" : { "pestphp/pest" : "^4.1.0", "symfony/var-dumper" : "^7.3.4" },
    "autoload" : { "psr-4" : { "CapsulesCodes\\PhpCsFixerCustomFixers\\" : "src/" } },
    "autoload-dev" : { "psr-4" : { "CapsulesCodes\\PhpCsFixerCustomFixers\\Tests\\" : "tests/" } },
    "config": { "allow-plugins": { "pestphp/pest-plugin": true } },
    "scripts" : { "test" : "vendor/bin/pest" }
}
